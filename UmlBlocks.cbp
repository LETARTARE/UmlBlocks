<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="UmlBlocks" />
		<Option pch_mode="2" />
		<Option compiler="gnu_gcc_compiler_w_sjlj_unwindings" />
		<Build>
			<Target title="default">
				<Option output="UmlBlocks" prefix_auto="1" extension_auto="1" />
				<Option type="3" />
				<Option compiler="gnu_gcc_compiler_w_sjlj_unwinding" />
				<Option host_application="$(#cb)\devel\codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-g" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DWX_PRECOMP" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="$(#cb)\include" />
					<Add directory="$(#cb)\sdk\wxscintilla\include" />
					<Add directory="$(#wx.include)" />
					<Add directory="$(#wx.lib)\gcc_dll\mswu" />
					<Add directory="$(#wxsf)\include" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
					<Add library="codeblocks" />
					<Add library="wxmsw28u" />
					<Add library="wxmsw28u_wxsf" />
					<Add directory="$(#cb)\devel" />
					<Add directory="$(#wx.lib)\gcc_dll" />
					<Add directory="$(#wxsf)\lib\vc_dll" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 UmlBlocks.zip manifest.xml" />
					<Add after="zip -j9 UmlBlocks.cbplugin UmlBlocks.dll UmlBlocks.zip" />
				</ExtraCommands>
			</Target>
		</Build>
		<Unit filename="UmlBlocks.cpp" />
		<Unit filename="UmlBlocks.h" />
		<Unit filename="UmlCanvas.cpp" />
		<Unit filename="UmlCanvas.h" />
		<Unit filename="UmlClass.cpp" />
		<Unit filename="UmlClass.h" />
		<Unit filename="UmlEditor.cpp" />
		<Unit filename="UmlEditor.h" />
		<Unit filename="UmlMemberFunc.cpp" />
		<Unit filename="UmlMemberFunc.h" />
		<Unit filename="UmlMemberGlobals.h" />
		<Unit filename="UmlMemberVar.cpp" />
		<Unit filename="UmlMemberVar.h" />
		<Unit filename="UmlNewEntityDialog.cpp" />
		<Unit filename="UmlNewEntityDialog.h" />
		<Unit filename="manifest.xml" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
